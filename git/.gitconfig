[user]
	name = askerry
	email = amy.skerry@gmail.com
[credential]
	helper = osxkeychain
[push]
	default = simple
[core]
	editor = emacs
	excludesfile = /Users/amyskerry/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
